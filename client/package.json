{
  "name": "till_excercise",
  "version": "1.0.0",
  "description": "Webpack2, React and Redux",
  "scripts": {
    "prestart": "babel-node devTools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node devTools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src DevTools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --require mock-local-storage --require isomorphic-fetch --timeout 15000 --reporter progress devTools/testSetup.js src/**/*.test.js src/components/**/*.test.js",
    "test:watch": "npm run test -- --watch"
  },
